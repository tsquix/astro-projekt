---
import Header from '../components/Header.astro';

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="description" content="Astro description" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="icon" type="image/svg+xml" href="/astro-projekt/favicon.svg" />

    <link rel="stylesheet" href="/astro-projekt/global.css">
    <link rel="stylesheet" href="/astro-projekt/output.css">
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
    <style>
     
    </style>
  </head>
  <body>
    <Header /> 
    <main>
      <slot /> 
    </main>
     <script src="/astro-projekt/script.js" is:inline></script>

    
  </body>
</html>
